@{
    ViewBag.Title = " Vendor Form Page";
    Layout = "";
}


@using DGSWeb.ViewModels
@using Generic.Data.Models
@using Microsoft.AspNetCore.Identity
@model VendorFormViewModel




<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="~/css/vendor/form.css">
    <link rel="apple-touch-icon" type="image/png" href="https://static.codepen.io/assets/favicon/apple-touch-icon-5ae1a0698dcc2402e9712f7d01ed509a57814f994c660df9f7a952f3060705ee.png">
    <meta name="apple-mobile-web-app-title" content="CodePen">
    <link rel="shortcut icon" type="image/x-icon" href="~/images/devops.jpg">
    <link rel="mask-icon" type="" href="https://static.codepen.io/assets/favicon/logo-pin-8f3771b1072e3c38bd662872f6b673a722f4b3ca2421637d5596661b4e2132cc.svg" color="#111">
    <title>DGS - Vendor Registration Form</title>


    <style>
    </style>
    <script>
        window.console = window.console || function (t) { };
    </script>
    <script>
        if (document.location.search.match(/type=embed/gi)) {
            window.parent.postMessage("resize", "*");
        }
    </script>
</head>

<div class="body">

    <div style="display:none;">
        <div class="card-body">
            <div class="chartjs-size-monitor" style="position: absolute; left: 0px; top: 0px; right: 0px; bottom: 0px; overflow: hidden; pointer-events: none; visibility: hidden; z-index: -1;"><div class="chartjs-size-monitor-expand" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:1000000px;height:1000000px;left:0;top:0"></div></div><div class="chartjs-size-monitor-shrink" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:200%;height:200%;left:0; top:0"></div></div></div>
            <canvas id="myAreaChart" width="1080" height="323" style="display: block; height: 259px; width: 864px;" class="chartjs-render-monitor"></canvas>
        </div>

        <div class="col-sm-8 my-auto">
            <div class="chartjs-size-monitor" style="position: absolute; left: 0px; top: 0px; right: 0px; bottom: 0px; overflow: hidden; pointer-events: none; visibility: hidden; z-index: -1;"><div class="chartjs-size-monitor-expand" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:1000000px;height:1000000px;left:0;top:0"></div></div><div class="chartjs-size-monitor-shrink" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:200%;height:200%;left:0; top:0"></div></div></div>
            <canvas id="myBarChart" width="447" height="223" class="chartjs-render-monitor" style="display: block; height: 179px; width: 358px;"></canvas>
        </div>
        <div class="card-body">
            <div class="chartjs-size-monitor" style="position: absolute; left: 0px; top: 0px; right: 0px; bottom: 0px; overflow: hidden; pointer-events: none; visibility: hidden; z-index: -1;"><div class="chartjs-size-monitor-expand" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:1000000px;height:1000000px;left:0;top:0"></div></div><div class="chartjs-size-monitor-shrink" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:200%;height:200%;left:0; top:0"></div></div></div>
            <canvas id="myPieChart" width="300" height="300" class="chartjs-render-monitor" style="display: block; height: 240px; width: 240px;"></canvas>
        </div>
    </div>

    <form asp-controller="Vendor" asp-action="VendorQueryUpdate" method="post" class="multistep-form" style="margin-bottom:0 !important;" enctype="multipart/form-data">
        <ul class="progressbar">
            <li class="active">Form Identification</li>
            <li class="">Supplier Identification</li>
            <li>Supplier Profile</li>
            <li>Business Experience</li>
            <li>Staff Strength & Competences</li>
            <li>Financials</li>
            <li>Quality Management</li>
            <li>HSE</li>
            <li>Additional Information</li>
        </ul>
        <!--===================================================Form Identification=============================================================-->
        <fieldset style="">
            <h2 class="fs-title">Form Identification (Information of individual Registering the enterprise)</h2>
            <h5>Fields marked (*) are mandatory</h5>
            <h3 class="fs-subtitle">This is step 1 of 9</h3>
            @*<div asp-validation-summary="" class="text-danger mt-4">

                </div>*@
            @if (Model.ModelError != "")
            {
                <div class="" style="margin-bottom:20px;color:red;">
                    @Model.ModelError
                </div>
            }
            <div class="row-form">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="FormIdentificationName">Name*</label>
                    </div>
                    <input asp-for="FormIdentificationName" class="form-input" id="" placeholder="Enter Name...">
                    <div id="validation" style="text-align: start;">
                        <span asp-validation-for="FormIdentificationName" class="text-danger"></span>
                    </div>
                </div>

                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="FormIdentificationPosition">Position*</label>
                    </div>
                    <input asp-for="FormIdentificationPosition" class="form-input" id="" placeholder="Position">
                    <div id="validation">
                        <span asp-validation-for="FormIdentificationPosition" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="row-form">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="FormIdentificationPhoneNumber">Phone Number*</label>
                    </div>
                    <input asp-for="FormIdentificationPhoneNumber" class="form-input" id="" placeholder="Enter Phone Number...">
                    <div id="validation">
                        <span asp-validation-for="FormIdentificationPhoneNumber" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="FormIdentificationWorkPhoneNumber">Work Phone Number*</label>
                    </div>
                    <input asp-for="FormIdentificationWorkPhoneNumber" class="form-input" id="" placeholder="Enter Work Phone Number...">
                    <div id="validation">
                        <span asp-validation-for="FormIdentificationWorkPhoneNumber" class="text-danger" style="color:red; font-size:small;"></span>
                    </div>
                </div>
            </div>

            <div class="row-form">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="FormIdentificationEmailAddress">Email (Important for status notification) *</label>
                    </div>
                    <input asp-for="FormIdentificationEmailAddress" class="form-input" id="" placeholder="Enter Email...">
                    <div id="validation">
                        <span asp-validation-for="FormIdentificationEmailAddress" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-1-of-2">

                    <div style="text-align:start !important;">
                        <label asp-for="FormIdentificationDate">Date*</label>
                    </div>
                    <div style="text-align:start !important;">
                        <input asp-for="FormIdentificationDate" class="form-input" id="" placeholder="Date">
                    </div>
                    <div id="validation">
                        <span asp-validation-for="FormIdentificationDate" class="text-danger"></span>
                    </div>

                </div>
            </div>
            <input type="button" name="dashboard" id="dashboard" class="action-button" value="Return to Dashboard">
            <input type="button" name="next" class="next action-button" value="Continue to Step 2">
        </fieldset>
        <!--===================================================Form Identification(end)=============================================================-->
        <!--===================================================Supplier Identification=============================================================-->
        <fieldset style="display: none; left: 50%; opacity: 0;">
            <h2 class="fs-title">Supplier Identification</h2>
            <h5>Fields marked (*) are mandatory</h5>
            <h3 class="fs-subtitle">This is step 2 of 9</h3>
            @if (Model.ModelError != "")
            {
                <div class="" style="margin-bottom:10px;color:red;">
                    @Model.ModelError
                </div>
            }
            <div class="row-form">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="SupplierIdentificationCompanyName">Company Name*</label>
                    </div>
                    <input asp-for="SupplierIdentificationCompanyName" class="form-input" id="" placeholder="Enter Company Name...">
                    @*<label for="applicationEmail" class="form-label">Email</label>*@
                    <div id="validation">
                        <span asp-validation-for="SupplierIdentificationCompanyName" class="text-danger" style="color:red; font-size:small;"></span>
                    </div>
                </div>
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="SupplierIdentificationCompanyRegNumber">Company Registration Number*</label>
                    </div>
                    <input asp-for="SupplierIdentificationCompanyRegNumber" class="form-input" id="" placeholder="Enter Registration Number...">
                    @*<label for="applicationMobilePhone" class="form-label">Mobile Phone</label>*@
                    <div id="validation">
                        <span asp-validation-for="SupplierIdentificationCompanyRegNumber" class="text-danger" style="color:red; font-size:small;"></span>
                    </div>
                </div>


            </div>
            <div class="row-form">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="CompanyIsForeign">Is it a foreign Company ?</label>
                        <div>(Check for companies outside Nigeria)*</div>
                    </div>
                    <div style="text-align:start;">
                        <input asp-for="CompanyIsForeign" class="form-input" style="height:25px;width:25px;" id="" placeholder="">
                    </div>
                    @*<label for="applicationEmail" class="form-label">Email</label>*@
                    @*<div id="validation">
                            <span asp-validation-for="CompanyIsForeign" class="text-danger" style="color:red; font-size:small;"></span>
                        </div>*@
                </div>
                <div class=" col-1-of-2 custom-file">
                    <!--asp-for tag helper is set to "SupplierIdentificationTaxClearanceCertificate" property.
                           "SupplierIdentificationTaxClearanceCertificate" property type is IFormFile
                           so at runtime asp.net core generates file upload control (input type=file). To
                           support multiple files upload include multiple attribute on the input element
                    -->
                    <div>
                        <div style="text-align:start !important;">
                            <label class="custom-file-label" style="">Tax Clearance Certificate*</label>
                        </div>
                        <input asp-for="SupplierIdentificationTaxClearanceCertificate" id="file" class="custom-file-input form-control" placeholder="">
                    </div>
                    <div id="validation">
                        <span asp-validation-for="SupplierIdentificationTaxClearanceCertificate" class="text-danger"></span>
                    </div>
                </div>


            </div>
            <div class="row-form">
                <div class="col-1-of-2 custom-file">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0" style="">Company Profile*</label>
                    </div>
                    <div style="text-align:start !important;">
                        <input asp-for="SupplierIdentificationCompanyProfile" id="file" class="custom-file-input form-control" placeholder="Click here to upload Company Profile">
                    </div>
                    <div id="validation"> <span asp-validation-for="SupplierIdentificationCompanyProfile" class="text-danger"></span></div>
                </div>
                <div class="col-1-of-2 custom-file">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0" style="">Corporate Affairs Commission Number</label>
                    </div>
                    <div style="text-align:start !important;">
                        <input asp-for="SupplierIdentificationCAC" id="file" class="custom-file-input form-control" placeholder="Input CAC Number">
                    </div>
                    <div id="validation"> <span asp-validation-for="CompanyIsForeign" class="text-danger"></span></div>
                </div>


            </div>
            <div>
                <div style="text-align:start !important;">
                    <label asp-for="SupplierIdentificationCompanyWebsiteUrl">Company Website URL</label>
                </div>
                <input asp-for="SupplierIdentificationCompanyWebsiteUrl" class="form-input" id="" placeholder="Company Website URL">
                <div><span asp-validation-for="SupplierIdentificationCompanyWebsiteUrl" class="text-danger"></span></div>
            </div>
            <div class="row-form">
                <div class="col-1-of-2 custom-file">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0">Bank Reference Letter</label>
                    </div>
                    <div style="text-align:start !important;">
                        <input asp-for="SupplierIdentificationBankReference" id="file" class="custom-file-input form-control" placeholder="">
                    </div>
                    <div id="validation"><span asp-validation-for="SupplierIdentificationBankReference" class="text-danger"></span></div>
                </div>
                <div class="col-1-of-2 custom-file">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0">Third Party Reference</label>
                    </div>
                    <div style="text-align:start !important;">
                        <input asp-for="SupplierIdentificationThirdPartyReference" id="file" class="custom-file-input form-control" placeholder="">
                    </div>
                    <div id="validation"><span asp-validation-for="SupplierIdentificationThirdPartyReference" class="text-danger"></span></div>
                </div>
            </div>
            <div class="row-form">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="SupplierIdentificationCompanyName"> Subsidiary Company Name 1</label>
                    </div>
                    <input asp-for="SupplierIdentificationSubsidiaryCompanyName1" class="form-input" id="" placeholder="Subsidiary Company Name 1">
                    @*<label for="applicationMobilePhone" class="form-label">Mobile Phone</label>*@
                    <div id="validation">
                        <span asp-validation-for="SupplierIdentificationSubsidiaryCompanyName1" class="text-danger" style="color:red; font-size:small;"></span>
                    </div>
                </div>
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="SupplierIdentificationSubsidiaryCompanyName2"> Subsidiary Company Name 2</label>
                    </div>
                    <input asp-for="SupplierIdentificationSubsidiaryCompanyName2" class="form-input" id="" placeholder="Subsidiary Company Name 2">
                    @*<label for="applicationEmail" class="form-label">Email</label>*@
                    <div id="validation">
                        <span asp-validation-for="SupplierIdentificationSubsidiaryCompanyName2" class="text-danger" style="color:red; font-size:small;"></span>
                    </div>
                </div>

            </div>
            <div class="row-form">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="SupplierIdentificationSubsidiaryCompanyName3">Subsidiary Company Name 3</label>
                    </div>
                    <input asp-for="SupplierIdentificationSubsidiaryCompanyName3" class="form-input" id="" placeholder="Subsidiary Company Name 3">
                    @*<label for="applicationMobilePhone" class="form-label">Mobile Phone</label>*@
                    <div id="validation">
                        <span asp-validation-for="SupplierIdentificationSubsidiaryCompanyName3" class="text-danger" style="color:red; font-size:small;"></span>
                    </div>
                </div>
            </div>
            <div>
                <div style="text-align:start !important;">
                    <label asp-for="SupplierIdentificationHeadOfficeAddress"> Company Head Office Address*</label>
                </div>
                <input asp-for="SupplierIdentificationHeadOfficeAddress" class="form-input" id="" placeholder="Head Office Address...">
                <div>
                    <span asp-validation-for="SupplierIdentificationHeadOfficeAddress" class="text-danger"></span>
                </div>
            </div>

            <div class="my-5"></div>
            <div class="row-form ">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="MainContactPersonName">Main Contact Person Name*</label>
                    </div>
                    <input asp-for="MainContactPersonName" class="form-input" id="applicationFirstName" placeholder="Enter Main Contact Name">
                    <div id="validation">
                        <span asp-validation-for="MainContactPersonName" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="MainContactPosition">Main Contact Position*</label>
                    </div>
                    <input asp-for="MainContactPosition" class="form-input" id="" placeholder="Enter Main Contact Position...">
                    <div id="validation">
                        <span asp-validation-for="MainContactPosition" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="row-form">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="MainContactPhoneNumber">Main Contact Phone Number*</label>
                    </div>
                    <input asp-for="MainContactPhoneNumber" class="form-input" id="" placeholder="Enter Main Contact Phone Number">
                    <div id="validation"><span asp-validation-for="MainContactPhoneNumber" class="text-danger"></span></div>
                </div>
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="MainContactWorkPhoneNumber">Main Contact Work Phone Number*</label>
                    </div>
                    <input asp-for="MainContactWorkPhoneNumber" class="form-input" id="" placeholder="Enter Main Contact Work Phone Number">
                    <div id="validation"><span asp-validation-for="MainContactWorkPhoneNumber" class="text-danger"></span></div>
                </div>
            </div>
            <div class="row-form">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label asp-for="MainContactEmailAddress">Main Contact Email*</label>
                    </div>
                    <input asp-for="MainContactEmailAddress" class="form-input" id="" placeholder="Main Contact Email">
                    <div id="validation"><span asp-validation-for="MainContactEmailAddress" class="text-danger"></span></div>
                </div>
            </div>


            <div class="my-5"></div>
            <div class="row-form ">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0">Third Party Reference Name</label>
                    </div>
                    <input asp-for="TprName" class="form-input" id="" placeholder=" Name">
                    <div id="validation"><span asp-validation-for="TprName" class="text-danger"></span></div>
                </div>
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0">Third Party Reference Email</label>
                    </div>
                    <input asp-for="TprEmailAddress" class="form-input" id="" placeholder=" Email">
                    <div id="validation"><span asp-validation-for="TprEmailAddress" class="text-danger"></span></div>
                </div>
            </div>
            <div class="row-form">
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0">Third Party Reference Phone Number</label>
                    </div>
                    <input asp-for="TprPhoneNumber" class="form-input" id="" placeholder="Enter Phone Number">
                    <div id="validation">
                        <span asp-validation-for="TprPhoneNumber" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-1-of-2">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0">Third Party Reference Work Phone Number</label>
                    </div>
                    <input asp-for="TprWorkPhoneNumber" class="form-input" id="" placeholder="Enter Work Phone Number">
                    <div id="validation"><span asp-validation-for="TprWorkPhoneNumber" class="text-danger"></span></div>
                </div>
            </div>
            <div>
                <div style="text-align:start !important;">
                    <label class="custom-file-label m-0 p-0">Third Party Reference Organization</label>
                </div>
                <input asp-for="TprOrganization" class="form-input" id="" placeholder="TPR Organization">
                <div><span asp-validation-for="TprOrganization" class="text-danger"></span></div>
            </div>
            <div style="text-align:start !important;">
                <label class="custom-file-label m-0 p-0">Third Party Reference Address</label>
            </div>
            <input asp-for="TprAddress" class="form-input" id="" placeholder="TPR Address">
            <div><span asp-validation-for="TprAddress" class="text-danger"></span></div>

            <input type="button" name="previous" class="previous action-button" value="Form Identification - Step 1">
            <input type="button" name="next" class="next action-button" value="Continue to step 3">
        </fieldset>
        <!--===================================================Supplier Identification(END)=============================================================-->
        <!--===================================================Supplier Profile=============================================================-->
        <fieldset>
            <h2 class="fs-title">Supplier Profile</h2>
            <h5>Fields marked (*) are mandatory</h5>
            <h3 class="fs-subtitle">This is step 3 of 9</h3>
            @if (Model.ModelError != "")
            {
                <div class="" style="margin-bottom:10px;color:red;">
                    @Model.ModelError
                </div>
            }
            <div style="text-align:start !important;">
                <label class="custom-file-label m-0 p-0" style="font-size:small;">Nature of Business*</label>
            </div>
            <textarea asp-for="NatureOfBusiness" class="form-input" placeholder="Nature of business..."></textarea>
            <div><span asp-validation-for="NatureOfBusiness" class="text-danger"></span></div>
            <div class="row-form">
                <div class="col-1-of-2 custom-file">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0" style="font-size:small;">Date of Creation*</label>
                    </div>
                    <div style="text-align:start !important;">
                        <input asp-for="DateofCreation" placeholder="">
                    </div>
                    <div id="validation"><span asp-validation-for="DateofCreation" class="text-danger"></span></div>
                </div>
                <div class="col-1-of-2 custom-file">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0" style="font-size:small;">Code of Conduct</label>
                    </div>
                    <div style="text-align:start !important;">
                        <input asp-for="CodeOfConduct" class="custom-file-input form-control" placeholder="">
                    </div>
                    <div><span asp-validation-for="CodeOfConduct" class="text-danger"></span></div>
                </div>
            </div>
            <div class="row-form " style="margin-top:10px;">
                <div class="col-1-of-2 custom-file">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0" style="font-size:small;">Organization Charts</label>
                    </div>
                    <div style="text-align:start !important;">
                        <input asp-for="SupplierProfileOrganizationCharts" id="file" class="custom-file-input form-control" placeholder="">
                    </div>
                    <div id="validation"><span asp-validation-for="SupplierProfileOrganizationCharts" class="text-danger"></span></div>
                </div>
                <div class="col-1-of-2 custom-file">
                    <div style="text-align:start !important;">
                        <label class="custom-file-label m-0 p-0" style="font-size:small;">Mission And Vision Statement</label>
                    </div>
                    <div style="text-align:start !important;">
                        <input asp-for="SupplierProfileMissionVisionStatement" id="file" class="custom-file-input form-control" placeholder="">
                    </div>
                    <div id="validation"><span asp-validation-for="SupplierProfileMissionVisionStatement" class="text-danger"></span></div>
                </div>

            </div>
            <div class="row-form" style="margin-top:10px;">
                <div class="col-2-of-4">
                    <div>
                        <label class="form-label">Ownership Main Share Holder 1</label>
                    </div>
                    <input asp-for="MainShareholder1" class="form-input" placeholder="Main Share Holder 1">
                    <div id="validation"><span asp-validation-for="MainShareholder1" class="text-danger"></span></div>
                </div>
                <div class="col-1-of-4">
                    <div>
                        <label class="form-label">Ownership Share Holding 1</label>
                    </div>
                    <input asp-for="Shareholding1" class="form-input" placeholder="Share Holding 1">
                    <div id="validation"><span asp-validation-for="Shareholding1" class="text-danger"></span></div>
                </div>
                <div class="col-1-of-4">
                    <div style="text-align:start !important;">
                        <label style="font-size:small;" asp-for="SupplierOwnershipCountryId1">Ownership Nationality 1</label>
                    </div>

                    @if (Model != null)
                    {
                        <div style="text-align:start !important;">
                            <select asp-for="SupplierOwnershipCountryId1" asp-items="Model.Countries.Select(x => new SelectListItem() { Value = x.CountryId.ToString(), Text = x.CountryName });">
                                <option value="">-Select-</option>
                            </select>
                        </div>

                    }
                    <div id="validation"><span asp-validation-for="SupplierOwnershipCountryId1" class="text-danger"></span></div>
                </div>

            </div>
            <div class="row-form" style="margin-top:10px;">
                <div class="col-2-of-4">
